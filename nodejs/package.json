{
  "name": "nodejs-workspace",
  "version": "1.0.0",
  "description": "Workspace of JavaScript applications",
  "main": "server.js",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "start:authentication-service": "npm start --workspace=authentication-service",
    "start:dev:authentication-service": "npm run start:dev --workspace=authentication-service"
  },
  "author": "Agustin Lanusse (@alanusse)",
  "license": "MIT",
  "devDependencies": {
    "eslint": "8.14.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.0",
    "eslint-plugin-promise": "6.0.0",
    "nodemon": "2.0.15"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "mongoose": "6.3.3",
    "mongoose-unique-validator": "3.0.0",
    "morgan": "1.10.0",
    "uuid": "8.3.2",
    "validator": "13.7.0",
    "joi": "17.6.0"
  }
}
